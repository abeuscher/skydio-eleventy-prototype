{% macro structuredData(meta, url, title) %}
  <script type="application/ld+json">
    {{ {
      '@context': 'http://schema.org',
      '@type': 'BreadcrumbList',
      itemListElement: [
        {
          '@type': 'ListItem',
          position: 1,
          item: {
            '@id': url,
            name: title,
          },
        },
      ],
    } | dump
    }}
  </script>
{% endmacro %}
