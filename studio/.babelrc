{
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env", {
      "modules": "commonjs",
      "targets": {
        "node": "8",
        "chrome": "59",
        "safari": "10",
        "firefox": "56",
        "edge": "14"
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ]
}
